{
  "name": "video-stitch-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.9.2",
    "chokidar": "2.0.2",
    "ffmpeg-static": "^2.3.0",
    "handbrake-js": "github:jpangelle/handbrake-js",
    "moment": "^2.22.2",
    "nodemon": "^1.18.4",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "2.0.4",
    "uuid": "^3.3.2",
    "video-stitch": "jpangelle/VideoStitch#master"
  },
  "author": "Jean-Paul Angelle and Mitchell Sistrunk",
  "build": {
    "appId": "video-stitch",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "src/utils/*",
      "!dist/",
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    },
    "asarUnpack": [
      "**/app/node_modules/ffmpeg-static/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "files": [
        "!node_modules/ffmpeg-static/bin/win32${/*}",
        "!node_modules/ffmpeg-static/bin/linux${/*}"
      ]
    },
    "win": {
      "files": [
        "!node_modules/ffmpeg-static/bin/linux${/*}",
        "!node_modules/ffmpeg-static/bin/darwin${/*}"
      ]
    },
    "linux": {
      "files": [
        "!node_modules/ffmpeg-static/bin/win32${/*}",
        "!node_modules/ffmpeg-static/bin/darwin${/*}"
      ]
    }
  },
  "license": "ISC",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "electron": "electron .",
    "prod": "build -c.extraMetadata.main=build/electron.js",
    "preprod": "npm run build",
    "prerelease": "npm run build"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "20.11.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "simple-node-logger": "^0.93.40",
    "foreman": "^3.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
